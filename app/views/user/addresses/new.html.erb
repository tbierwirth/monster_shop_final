<%= form_for @address, url: user_addresses_path do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.label :alias %>
  <%= f.text_field :alias %>

  <%= f.submit %>
<% end %>
